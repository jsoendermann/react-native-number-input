{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "interface-name": [
      false
    ],
    "no-consecutive-blank-lines": false,
    "member-ordering": [
      false
    ],
    "member-access": [
      false
    ],
    "class-name": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "no-unused-expression": [
      false
    ]
  }
}